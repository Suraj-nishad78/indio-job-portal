<style>
    body{
        background-color: #181743;
    }
</style>
<main>
    <div id="main-content">
        <div id="left-main-content">
            <h1>A comprehensive platform for reliable</h1> 
            <h2 id="fadded-text">Job opportunities and internships.</h2>
            <p>Get Discovered. Get Hired.</p>
            <div id="buttons-main">
                <ul>
                    <!-- <li><a href="/jobs">I'm A job Seeker</a></li> -->
                    <% if(user || app){ %>
                        <li><button id="signup-app" disabled>I'm A job Seeker</button></li>
                        <li><button id="signup-visible" disabled>I'm A Recruiter</button></li>
                    <% } else { %>
                        <li><button id="signup-app">I'm A job Seeker</button></li>
                        <li><button id="signup-visible">I'm A Recruiter</button></li>
                    <% } %>
                </ul>
            </div>
        </div>
        <div id="right-main-content">
            <img src="https://easily-deploy.onrender.com/images/banner.png">
        </div>
    </div>
    <!------- Recruiter's ------->
    <div id="signup-form" class="hidden">
        <div id="title-signup">
            <p>Indio Jobs! Recruiter's SignUp</p>
            <a id="signup-hide" href="#">X</a>
        </div>
        <hr>
        <div id="signup-input" >
            <form action="/recruiter" method="post">
                <label for="recruiter-name">Name</label>
                <input id="recruiter-name" type="text" name="name" placeholder="Enter your name" required style="width: 19em;">
                <br>
                <label for="recruiter-email">Email</label>
                <input id="recruiter-email" type="email" name="email" placeholder="Enter your email" required style="width: 19em;">
                <br>
                <label for="recruiter-password">Password</label>
                <input id="recruiter-password" type="password" name="password" placeholder="Enter your password" required autocomplete="off" style="width: 17.1em;">
                <br>
                <button type="submit" onclick="recruiterSignup()">Register</button>
            </form>
        </div>
        <hr>
        <div id="signup-to-login">
            <div id="signup-login">
                <p>Already registered?</p>
                <a id="signup-login-page" href="#">Login</a>
            </div>
            <div id="social-link">
                <img src="data:image/png;base64,">
                <img src="https://cdn4.iconfinder.com/data/icons/social-media-logos-6/512/62-instagram-512.png">
                <img src="data:image/png;base64,">
                <img src="data:image/png;base64,">
                <img src="data:image/png;base64,">
            </div>
        </div>
    </div>
    <div id="login-form" class="hidden">
        <div id="title-login">
            <p>Indio Jobs! Recruiter's Login</p>
            <a class="login-hide" id="login-hide" href="#">X</a>
        </div>
        <hr>
        <div id="login-input" >
            <form action="/login" method="post">
                <label for="login-email">Email</label>
                <input id="login-email" type="email" name="email" placeholder="Enter your email" required style="width: 19em;">
                <br>
                <label for="login-password">Password</label>
                <input id="login-password" type="password" name="password" placeholder="Enter your password" required autocomplete="off" style="width: 17.1em;">
                <br>
                <button type="submit">Login</button>
            </form>
        </div>
        <hr>
        <div id="login-to-signup">
            <div id="login-signup">
                <p>Don't have an account?</p>
                <a id="login-signup-page" href="#">Signup</a>
            </div>
            <div id="close-login-form">
                <button class="login-hide">Close</button>
            </div>
        </div>
    </div>
    <!------- Applicant's ------->
    <div id="signup-form-app" class="hidden">
        <div id="title-signup">
            <p>Indio Jobs! Applicant's SignUp</p>
            <a id="signup-hide-app" href="#">X</a>
        </div>
        <hr>
        <div id="signup-input" >
            <form action="/Signup/applicants" method="post">
                <label for="app-name">Name</label>
                <input id="app-name" type="text" name="name" placeholder="Enter your name" required style="width: 19em;">
                <br>
                <label for="app-email">Email</label>
                <input id="app-email" type="email" name="email" placeholder="Enter your email" required style="width: 19em;">
                <br>
                <label for="app-password">Password</label>
                <input id="app-password" type="password" name="password" placeholder="Enter your password" required autocomplete="off" style="width: 17.1em;">
                <br>
                <button type="submit" onclick="accountSignup()">Register</button>
            </form>
        </div>
        <hr>
        <div id="signup-to-login">
            <div id="signup-login">
                <p>Already registered?</p>
                <a id="signup-login-page-app" href="#">Login</a>
            </div>
            <div id="social-link">
                <img src="data:image/png;base64,">
                <img src="https://cdn4.iconfinder.com/data/icons/social-media-logos-6/512/62-instagram-512.png">
                <img src="data:image/png;base64,">
                <img src="data:image/png;base64,">
                <img src="data:image/png;base64,">
            </div>
        </div>
    </div>
    <div id="login-form-app" class="hidden">
        <div id="title-login">
            <p>Indio Jobs! Applicant's Login</p>
            <a class="login-hide-app" id="login-hide" href="#">X</a>
        </div>
        <hr>
        <div id="login-input" >
            <form action="/login/applicant" method="post">
                <label for="login-email">Email</label>
                <input id="login-email" type="email" name="email" placeholder="Enter your email" required style="width: 19em;">
                <br>
                <label for="login-password">Password</label>
                <input id="login-password" type="password" name="password" placeholder="Enter your password" required autocomplete="off" style="width: 17.1em;">
                <br>
                <button type="submit">Login</button>
            </form>
        </div>
        <hr>
        <div id="login-to-signup">
            <div id="login-signup">
                <p>Don't have an account?</p>
                <a id="login-signup-page-app" href="#">Signup</a>
            </div>
            <div id="close-login-form">
                <button class="login-hide-app">Close</button>
            </div>
        </div>
    </div>
</main>